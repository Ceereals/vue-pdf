{
  "name": "@ceereals/vue-pdf",
  "version": "0.2.1",
  "description": "Vue PDF - Create PDF documents using Vue components",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ceereals/vue-pdf.git"
  },
  "homepage": "https://vue-pdf.org",
  "keywords": [
    "vue",
    "pdf",
    "vue-pdf",
    "renderer"
  ],
  "type": "module",
  "main": "dist/node/index.js",
  "license": "MIT",
  "author": "Ceereals",
  "files": [
    "dist",
    "dist/types"
  ],
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "browser": "./dist/dom/index.js",
      "default": "./dist/node/index.js"
    }
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "coverage:ui": "vitest --coverage --ui",
    "format": "biome format --write ",
    "lint": "biome lint --write",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@react-pdf/fns": "^3.1.2",
    "@react-pdf/font": "^4",
    "@react-pdf/layout": "^4",
    "@react-pdf/pdfkit": "^4",
    "@react-pdf/primitives": "^4",
    "@react-pdf/render": "^4",
    "@vue/devtools-api": "^8",
    "@vueuse/core": "^13.6.0",
    "defu": "^6.1.4",
    "patch-package": "^8.0.0",
    "vue": "^3"
  },
  "peerDependencies": {
    "vue": "^3"
  },
  "devDependencies": {
    "@react-pdf/types": "^2.5.0",
    "@types/node": "^22.18.7",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vitest/web-worker": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^19.0.2",
    "memfs": "^4.23.0",
    "rollup-plugin-dts": "^6.2.1",
    "typescript": "~5.8.2",
    "vite": "^7.0.6",
    "vite-plugin-dts": "^4.5.4",
    "vitepress": "^2.0.0-alpha.12",
    "vitepress-plugin-group-icons": "^1.6.1",
    "vitest": "^3.2.4"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.46.1"
  }
}
