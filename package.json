{
  "name": "@ceereals/vue-pdf",
  "version": "0.0.10",
  "description": "Vue PDF - Create PDF documents using Vue components",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ceereals/vue-pdf.git"
  },
  "homepage": "https://vue-pdf.org",
  "keywords": [
    "vue",
    "pdf",
    "vue-pdf",
    "renderer"
  ],
  "type": "module",
  "main": "dist/node/index.js",
  "license": "MIT",
  "author": "Ceereals",
  "files": [
    "dist",
    "dist/types"
  ],
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "browser": "./dist/dom/index.js",
      "default": "./dist/node/index.js"
    },
    "./fns": {
      "types": "./dist/fns/index.d.ts",
      "import": "./dist/fns/index.js"
    },
    "./vite": {
      "types": "./dist/types/vue-pdf-plugin.d.ts",
      "import": "./dist/vue-pdf-plugin.js"
    }
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "npm run build:fns && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "coverage:ui": "vitest --coverage --ui",
    "format": "biome format --write ",
    "lint": "biome lint --write",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "build:fns": "cd src/react-pdf/fns && npm run build"
  },
  "dependencies": {
    "@react-pdf/font": "^4",
    "@react-pdf/layout": "^4",
    "@react-pdf/pdfkit": "^4",
    "@react-pdf/primitives": "^4",
    "@react-pdf/render": "^4",
    "@vue/devtools-api": "^7.5.4",
    "@vueuse/core": "^10.11.1",
    "defu": "^6.1.4",
    "vue": "^3.4.38"
  },
  "peerDependencies": {
    "vue": "^3.4.38"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@ceereals/vue-pdf": "^0.0.10",
    "@react-pdf/types": "^2.5.0",
    "@types/node": "^22.5.0",
    "@types/react": "^18.3.18",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vitest/coverage-v8": "^3.0.7",
    "@vitest/ui": "^3.0.7",
    "@vitest/web-worker": "^3.0.7",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^17.1.8",
    "memfs": "^4.17.0",
    "pdfjs-dist": "^5.0.375",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.1.1",
    "typescript": "~5.7.3",
    "vite": "^6.2.3",
    "vite-plugin-dts": "^4.0.3",
    "vite-plugin-static-copy": "^2.3.0",
    "vitepress": "^1.6.3",
    "vitepress-plugin-group-icons": "^1.3.6",
    "vitest": "^3.0.7"
  }
}
